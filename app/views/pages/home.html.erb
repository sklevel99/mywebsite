<!-- HEADER -->
<body>
  <header class="header" id="header">
    <nav class="nav container">
      <a href="#" class="nav_logo">Khooshal</a>
      <div class="nav_menu" id="nav-menu">
        <ul class="nav_list grid">
          <li class="nav_item">
            <a href="#home" class="nav_link"></a>
            <i class="fas fa-home nav_icon"></i> Home
          </li>

          <li class="nav_item">
            <a href="#about" class="nav_link"></a>
            <i class="far fa-user nav_icon"></i> About
          </li>

          <li class="nav_item">
            <a href="#skills" class="nav_link"></a>
            <i class="far fa-file nav_icon"></i> Skills
          </li>

          <li class="nav_item">
            <a href="#portfolio" class="nav_link"></a>
            <i class="fas fa-layer-group nav_icon"></i> Portfolio
          </li>

          <li class="nav_item">
            <a href="#contact" class="nav_link"></a>
            <i class="far fa-comment-dots nav_icon"></i> Contact
          </li>
        </ul>
        <i class="fas fa-times nav_close" id="nav-close"></i>
      </div>

      <div class="nav_btns">
        <div class="nav_toggle" id="nav-toggle">
          <i class="fas fa-list"></i>
        </div>
      </div>
    </nav>
  </header>
  <main class="main">
    <section class="home section" id="home">
      <div class="home_container container grid">
        <div class="home_content grid">
          <div class="home_social">
            <a href="https://www.linkedin.com/in/khooshal-seechurn/" target = "_blank" class="home_social-icon">
              <i class="fab fa-linkedin"></i>
            </a>

             <a href="https://www.facebook.com/people/Khooshal-Seechurn/100005124289494/" target = "_blank" class="home_social-icon">
              <i class="fab fa-facebook-square"></i>
            </a>

             <a href="https://github.com/sklevel99" target = "_blank" class="home_social-icon">
              <i class="fab fa-github"></i>
            </a>

          </div>
          <div class="home_img">
            <svg class="home_blob" viewBox="0 0 200 187" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
              <mask id="mask0" mask-type="alpha">
                <path d="M190.312 36.4879C206.582 62.1187 201.309 102.826 182.328 134.186C163.346 165.547
                130.807 187.559 100.226 186.353C69.6454 185.297 41.0228 161.023 21.7403 129.362C2.45775
                97.8511 -7.48481 59.1033 6.67581 34.5279C20.9871 10.1032 59.7028 -0.149132 97.9666
                0.00163737C136.23 0.303176 174.193 10.857 190.312 36.4879Z"/>
              </mask>
              <g mask="url(#mask0)">
                <path d="M190.312 36.4879C206.582 62.1187 201.309 102.826 182.328 134.186C163.346
                165.547 130.807 187.559 100.226 186.353C69.6454 185.297 41.0228 161.023 21.7403
                129.362C2.45775 97.8511 -7.48481 59.1033 6.67581 34.5279C20.9871 10.1032 59.7028
                -0.149132 97.9666 0.00163737C136.23 0.303176 174.193 10.857 190.312 36.4879Z"/>
                <%= image_tag 'my.png', class: "home_blob-img"%>
                <!-- <image class="home_blob-img" x='12' y='18' xlink:href="../../assets/images/my.png"> -->
              </g>
            </svg>
          </div>
          <div class="home_data">
            <h1 class="home_title">
              Hi, I am Khooshal
            </h1>
            <h3 class="home_subtitle">
              A Developer and Financier
            </h3>
            <p class="home_description">
              High level of experience in web design and finance
            </p>
            <a href="#contact" class="button button-flex">
              CONTACT ME <i class="far fa-envelope-open button-icon"></i>
            </a>
          </div>
        </div>
        <div class="home_scroll">
          <a href="#about" class="home_scroll-button button-flex"></a>
          <i class="fas fa-mouse home_scroll-mouse"></i>
          <span class="home_scroll-name">
            SCROLL DOWN
          </span>
          <i class="fas fa-arrow-down home_scroll-arrow"></i>
        </div>
      </div>
    </section>
    <!-- ABOUT ME -->
    <section class="about section" id="about">
      <h2 class="section_title text-center">
        ABOUT ME
      </h2>
      <div class="about_container container grid">
        <div class="about_data">
          <p class="about_description">Web developer and Finance guy</p>
          <div class="about_info">
            <div>
              <span class="about_info-title"> 01+ </span>
              <span class="about_info-name">Years <br> experience</span>
            </div>

            <div>
              <span class="about_info-title"> 5+ </span>
              <span class="about_info-name"> Completed <br> projects </span>
            </div>

            <div>
              <span class="about_info-title"> 03+ </span>
              <span class="about_info-name"> Companies <br> worked </span>
            </div>
          </div>
        </div>
      </div>
    </section>
    <!-- SKILLS -->
    <section class="skills section" id="skills">
      <h2 class="section_title text-center">SKILLS</h2>
      <span class="section_subtitle text-center">My technical level</span>
      <div class="skills_container container grid">
        <div>
          <div class="skills_content skills_open">
            <div class="skills_header">
              <i class="fas fa-code skills_icon"></i>
              <div>
                <h3 class="skills_title">Frontend Developer</h3>
                <span class="skills_subtitle">More than 1 year</span>
              </div>
              <i class="fas fa-arrow-down skills_arrow"></i>
            </div>

            <div class="skills_list grid">
              <div class="skills_data">
                <div class="skills_title">
                  <h4 class="skills_name">HTML</h4>
                  <span class="skills_number">90%</span>
                </div>
                <div class="skills_bar">
                  <span class="skills_percentage skills_html"></span>
                </div>
              </div>

              <div class="skills_data">
                <div class="skills_title">
                  <h4 class="skills_name">CSS</h4>
                  <span class="skills_number">90%</span>
                </div>
                <div class="skills_bar">
                  <span class="skills_percentage skills_css"></span>
                </div>
              </div>

              <div class="skills_data">
                <div class="skills_title">
                  <h4 class="skills_name">Javascript</h4>
                  <span class="skills_number">60%</span>
                </div>
                <div class="skills_bar">
                  <span class="skills_percentage skills_js"></span>
                </div>
              </div>
            </div>
          </div>
              <!-- Skill 2 -->
          <div class="skills_content skills_close">
            <div class="skills_header">
              <i class="fas fa-code-branch skills_icon"></i>
              <div>
                <h3 class="skills_title">Backend Developer</h3>
                <span class="skills_subtitle">More than 1 year</span>
              </div>
              <i class="fas fa-arrow-down skills_arrow"></i>
            </div>

            <div class="skills_list grid">
              <div class="skills_data">
                <div class="skills_title">
                  <h4 class="skills_name">Ruby</h4>
                  <span class="skills_number">90%</span>
                </div>
                <div class="skills_bar">
                  <span class="skills_percentage skills_ruby"></span>
                </div>
              </div>

              <div class="skills_data">
                <div class="skills_title">
                  <h4 class="skills_name">R</h4>
                  <span class="skills_number">70%</span>
                </div>
                <div class="skills_bar">
                  <span class="skills_percentage skills_r"></span>
                </div>
              </div>

              <div class="skills_data">
                <div class="skills_title">
                  <h4 class="skills_name">SQL</h4>
                  <span class="skills_number">55%</span>
                </div>
                <div class="skills_bar">
                  <span class="skills_percentage skills_sql"></span>
                </div>
              </div>
            </div>
          </div>
              <!-- Skills 3 -->
          <div class="skills_content skills_close">
            <div class="skills_header">
              <i class="fas fa-ruler-combined skills_icon"></i>
              <div>
                <h3 class="skills_title">Designer</h3>
                <span class="skills_subtitle">More than 1 year</span>
              </div>
              <i class="fas fa-arrow-down skills_arrow"></i>
            </div>

            <div class="skills_list grid">
              <div class="skills_data">
                <div class="skills_title">
                  <h4 class="skills_name">Figma</h4>
                  <span class="skills_number">90%</span>
                </div>
                <div class="skills_bar">
                  <span class="skills_percentage skills_figma"></span>
                </div>
              </div>

              <div class="skills_data">
                <div class="skills_title">
                  <h4 class="skills_name">Photoshop</h4>
                  <span class="skills_number">70%</span>
                </div>
                <div class="skills_bar">
                  <span class="skills_percentage skills_photoshop"></span>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>
  </main>
</body>








